<android.support.design.widget.CoordinatorLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    tools:context="org.geospaces.schas.UploadData">

    <RelativeLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:weightSum="1"
        android:id="@+id/uploaded"
        android:background="@color/ColorLightPrimary"
        >

        <fragment xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:tools="http://schemas.android.com/tools"
            android:layout_width="match_parent"
            android:layout_height="273dp"
            class="org.geospaces.schas.Fragments.GoogleMaps"
            android:id="@+id/fragment"
            android:layout_alignParentTop="true"
            android:layout_above="@+id/linearLayout2" />

<!--
        <EditText
            style="?android:attr/buttonStyleSmall"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="textMultiLine"
            android:ems="10"
            android:id="@+id/statusText"
            android:layout_gravity="center_horizontal"
            android:layout_weight="0.5"
            android:background="@color/ColorLightPrimary"
            android:maxLines="1000"
            android:editable="false"
            android:enabled="true"
            android:text="Status Text"
            android:clickable="false" />-->

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/linearLayout3"
            android:layout_above="@+id/linearLayout4"
            android:layout_alignParentStart="true"
            android:visibility="invisible"
            android:layout_alignTop="@+id/linearLayout4">

            <Button
                style="?android:attr/buttonStyleSmall"
                android:layout_width="0dip"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:text="MILD\nATTACK"
                android:id="@+id/attack1"
                android:layout_toRightOf="@+id/uploaded"
                android:layout_toEndOf="@+id/uploaded"
                android:layout_marginBottom="-5dp"
                android:layout_marginLeft="-8dp"
                android:layout_marginRight="-8dp"
                />

            <Button
                style="?android:attr/buttonStyleSmall"
                android:layout_width="0dip"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:text="MEDIUM\nATTACK"
                android:id="@+id/attack2"
                android:layout_below="@+id/graphButton"
                android:layout_alignLeft="@+id/textView2"
                android:layout_alignStart="@+id/textView2"
                android:layout_marginBottom="-5dp"
                android:layout_marginLeft="-8dp"
                android:layout_marginRight="-8dp"
                />

            <Button
                style="?android:attr/buttonStyleSmall"
                android:layout_width="0dip"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:text="SEVERE\nATTACK"
                android:id="@+id/attack3"
                android:layout_marginBottom="-5dp"
                android:layout_marginLeft="-8dp"
                android:layout_marginRight="-8dp"
                />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:id="@+id/linearLayout4"
            android:layout_alignParentStart="true"
            android:visibility="invisible"
            android:layout_alignParentBottom="true">

            <Button
                style="?android:attr/buttonStyleSmall"
                android:layout_width="0dip"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:text="I USED\nInhaler"
                android:id="@+id/inhlaerButton"
                android:layout_toRightOf="@+id/uploaded"
                android:layout_toEndOf="@+id/uploaded"
                android:layout_marginBottom="-5dp"
                android:layout_marginLeft="-8dp"
                android:layout_marginRight="-8dp"
                />

            <Button
                style="?android:attr/buttonStyleSmall"
                android:layout_width="0dip"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:text="Status"
                android:id="@+id/updateStatus"
                android:layout_below="@+id/graphButton"
                android:layout_alignLeft="@+id/textView2"
                android:layout_alignStart="@+id/textView2"
                android:visibility="invisible"/>

            <Button

                style="?android:attr/buttonStyleSmall"
                android:layout_width="0dip"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:text="I USED\nPeakFlow"
                android:id="@+id/PFMButton"
                android:layout_below="@+id/graphButton"
                android:layout_alignLeft="@+id/textView2"
                android:layout_alignStart="@+id/textView2"
                android:layout_marginBottom="-5dp"
                android:layout_marginLeft="-8dp"
                android:layout_marginRight="-8dp"
                />


        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/linearLayout2"
            android:layout_alignParentStart="true"
            android:layout_alignParentBottom="true">


            <Button
                android:layout_width="0dip"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:text="Start"
                android:id="@+id/homeButton"
                android:layout_toRightOf="@+id/uploaded"
                android:layout_toEndOf="@+id/uploaded"
                android:background="#ff00ff00" />

            <Button
                android:layout_width="0dip"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:text="RESET\nALL"
                android:id="@+id/resetButton"
                />
            <Button
                android:layout_width="0dip"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:text="Upload"
                android:id="@+id/graphButton" />
        </LinearLayout>

        <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
            android:layout_alignParentStart="true"
            android:layout_above="@+id/linearLayout2">

        <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:gravity="bottom|right|end"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:weightSum="1">

                <android.support.design.widget.FloatingActionButton
                android:id="@+id/map"
                android:src="@drawable/ic_map"
                app:elevation="8dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="24dp"
                android:layout_marginTop="16dp"
                app:fabSize="mini"
                app:rippleColor="@android:color/white"
                android:visibility="invisible"
                app:pressedTranslationZ="12dp"
                app:borderWidth="0dp"
                />

            <android.support.design.widget.FloatingActionButton
                android:id="@+id/test6"
                android:src="@drawable/ic_medicine"
                app:elevation="8dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="24dp"
                android:layout_marginTop="16dp"
                app:fabSize="mini"
                app:rippleColor="@android:color/white"
                android:visibility="invisible"
                app:pressedTranslationZ="12dp"
                app:borderWidth="0dp"
                />

            <android.support.design.widget.FloatingActionButton
                android:id="@+id/test5"
                android:src="@drawable/ic_inhaler"
                app:elevation="8dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="24dp"
                android:layout_marginTop="16dp"
                app:fabSize="mini"
                app:rippleColor="@android:color/white"
                android:visibility="invisible"
                app:pressedTranslationZ="12dp"
                app:borderWidth="0dp"
                />

            <android.support.design.widget.FloatingActionButton
                android:id="@+id/test4"
                android:src="@drawable/ic_peakflow"
                app:elevation="8dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="24dp"
                android:layout_marginTop="16dp"
                app:fabSize="mini"
                app:rippleColor="@android:color/white"
                android:visibility="invisible"
                app:pressedTranslationZ="12dp"
                app:borderWidth="0dp"
                />



            <android.support.design.widget.FloatingActionButton
                android:id="@+id/test3"
                android:src="@drawable/ic_heart3"
                app:elevation="8dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="24dp"
                android:layout_marginTop="16dp"
                app:fabSize="mini"
                app:rippleColor="@android:color/white"
                android:visibility="invisible"
                app:pressedTranslationZ="12dp"
                app:borderWidth="0dp"
                />

            <android.support.design.widget.FloatingActionButton
                android:id="@+id/test2"
                android:src="@drawable/ic_heart2"
                app:elevation="8dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="24dp"
                android:layout_marginTop="16dp"
                app:fabSize="mini"
                app:rippleColor="@android:color/white"
                android:visibility="invisible"
                app:pressedTranslationZ="12dp"
                app:borderWidth="0dp"
                />

            <android.support.design.widget.FloatingActionButton
                android:id="@+id/test1"
                android:src="@drawable/ic_heart"
                app:elevation="8dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="24dp"
                android:layout_marginTop="16dp"
                app:fabSize="mini"
                app:rippleColor="@android:color/white"
                android:visibility="invisible"
                app:pressedTranslationZ="12dp"
                app:borderWidth="0dp"
                />

            <android.support.design.widget.FloatingActionButton
                android:id="@+id/menu_button"
                android:src="@drawable/ic_heart_white"
                app:elevation="8dp"
                android:layout_width="76dp"
                android:layout_height="wrap_content"
                android:layout_margin="16dp"
                app:borderWidth="0dp"
                app:rippleColor="@android:color/white"
                app:pressedTranslationZ="12dp"
                android:layout_weight="0.04" />
        </LinearLayout>
    </RelativeLayout>
    </RelativeLayout>


</android.support.design.widget.CoordinatorLayout>
